  <body>
      <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        <div class="collapse navbar-collapse" id="navbarColor02">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <%= link "Pair", to: Routes.page_path(@conn, :index), class: "nav-link" %>
            </li>
            <li class="nav-item">
              <%= link "Challenges", to: Routes.challenge_index_path(@conn, :index), class: "nav-link" %>
            </li>
          </ul>
          <ul class="navbar-nav ml-auto">
            <%= if @current_user do %>
              <li class="nav-item active"><span class="nav-link">Hi, <%= @current_user.email %></span></li>
              <li class="nav-item active"><%= link "Settings", to: Routes.user_settings_path(@conn, :edit), class: "nav-link" %></li>
              <li class="nav-item active"><%= link "Logout", to: Routes.user_session_path(@conn, :delete), method: :delete, class: "nav-link" %></li>
              <%= if function_exported?(Routes, :live_dashboard_path, 2) do %>
                <li class="nav-item active"><%= link "LiveDashboard", to: Routes.live_dashboard_path(@conn, :home), class: "nav-link" %></li>
              <% end %>
            <% else %>
              <li class="nav-item active"><%= link "Register", to: Routes.user_registration_path(@conn, :new), class: "nav-link" %></li>
              <li class="nav-item active"><%= link "Login", to: Routes.user_session_path(@conn, :new), class: "nav-link" %></li>
            <% end %>
          </ul>
        </div>
      </nav>
      <main role="main" class="container" style="margin-top: 5%;">
        <%= if get_flash(@conn, :info) do %>
          <p class="alert alert-info" role="alert"><%= get_flash(@conn, :info) %></p>
        <% end %>
        <%= if  get_flash(@conn, :error) do %>
          <p class="alert alert-danger" role="alert"><%= get_flash(@conn, :error) %></p>
        <% end %>
        <%= @inner_content %>
      </main>
  </body>
